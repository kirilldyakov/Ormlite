<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.user.ormlite">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".Logo_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main_" />
        <activity android:name=".graph.Graph_Durty_" />
        <activity android:name=".graph.Graph_Power_" />
        <activity android:name=".graph.Graph_Energy_" />
        <activity android:name=".actSettings_" />
        <activity android:name=".properties.PropertiesList_" />
        <activity android:name=".properties.PropFilter_" />

    </application>

</manifest>